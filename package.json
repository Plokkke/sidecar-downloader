{
  "name": "node-ts-downloader",
  "version": "1.3.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "npm run lint -- --fix",
    "start": "nest start --env-file .env",
    "start:local": "npm run start -- --entryFile local",
    "start:dev": "npm run start -- --watch",
    "start:debug": "npm run start:dev -- --debug",
    "start:prod": "node dist/main",
    "validate": "npm run lint && npm run build && npm run test:cov",
    "test": "npm run test:unit && npm run test:e2e",
    "test:cov": "npm run test:unit:cov && npm run test:e2e:cov && npm run merge-cov",
    "test:unit": "jest --config ./test/jest-unit.js",
    "test:unit:cov": "npm run test:unit -- --coverage",
    "test:e2e": "jest --runInBand --forceExit --config ./test/jest-e2e.js",
    "test:e2e:cov": "npm run test:e2e -- --coverage",
    "merge-cov": "npx istanbul-merge --out ./reports/tests/unified/coverage/coverage-final.json ./reports/tests/e2e/coverage/coverage-final.json ./reports/tests/unit/coverage/coverage-final.json; npx istanbul report --include ./reports/tests/unified/coverage/coverage-final.json --dir reports/tests/unified/coverage/lcov-report html"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.12",
    "@nestjs/config": "^4.0.1",
    "@nestjs/platform-express": "^11.0.12",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/core": "^11.0.12",
    "axios": "^1.8.4",
    "lodash": "^4.17.21",
    "luxon": "^3.6.0",
    "nest-winston": "^1.10.2",
    "nestjs-zod": "^4.3.1",
    "reflect-metadata": "^0.2.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.5",
    "@nestjs/schematics": "^11.0.2",
    "@nestjs/testing": "^11.0.12",
    "@stryker-mutator/core": "^8.7.1",
    "@stryker-mutator/jest-runner": "^8.7.1",
    "@stryker-mutator/typescript-checker": "^8.7.1",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.16",
    "@types/luxon": "^3.4.2",
    "@types/node": "^22.13.14",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "docker-compose": "^1.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.5",
    "is-port-reachable": "^4.0.0",
    "jest": "^29.7.0",
    "nock": "^14.0.1",
    "prettier": "^3.5.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.0",
    "ts-jest": "^29.3.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
